<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
    <system.serviceModel>
        <bindings>
            <netNamedPipeBinding>
                <binding name="NetNamedPipeBinding_ICheckersService" />
            </netNamedPipeBinding>
            <netTcpBinding>
                <binding name="NetTcpBinding_ICheckersService" />
            </netTcpBinding>
        </bindings>
        <client>
            <endpoint address="net.pipe://localhost/CheckersAppServer" binding="netNamedPipeBinding"
                bindingConfiguration="NetNamedPipeBinding_ICheckersService"
                contract="CheckersServiceReference.ICheckersService" name="NetNamedPipeBinding_ICheckersService">
                <identity>
                    <userPrincipalName value="ASh-PC\ASh" />
                </identity>
            </endpoint>
            <endpoint address="net.tcp://localhost:5528/CheckersAppServer"
                binding="netTcpBinding" bindingConfiguration="NetTcpBinding_ICheckersService"
                contract="CheckersServiceReference.ICheckersService" name="NetTcpBinding_ICheckersService">
                <identity>
                    <userPrincipalName value="ASh-PC\ASh" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>